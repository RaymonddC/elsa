{
  "name": "elsa-wallet-analyzer",
  "version": "1.0.0",
  "description": "AI-powered Bitcoin & Ethereum wallet analyzer with Glass Box transparency",
  "main": "dist/index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\" --names \"backend,frontend\" --prefix-colors \"cyan,magenta\"",
    "dev:backend": "tsx watch src/index.ts",
    "dev:frontend": "cd frontend && npm run dev",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "generate-sample": "tsx scripts/generate_sample_data.ts",
    "init-index": "tsx scripts/init_elasticsearch.ts"
  },
  "keywords": [
    "elasticsearch",
    "ai",
    "bitcoin",
    "ethereum",
    "blockchain",
    "wallet",
    "etherscan",
    "openai",
    "agent"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@elastic/elasticsearch": "^8.11.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "google-auth-library": "^10.5.0",
    "jsonwebtoken": "^9.0.3",
    "openai": "^4.24.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "concurrently": "^9.2.1",
    "eslint": "^8.56.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
